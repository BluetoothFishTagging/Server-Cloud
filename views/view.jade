mixin FileFormat(file)
    tr
        td
            a(href=file.path, download) #{file.path}
        td
            p=file.description

html(lang="en")
    head
        title testMySQL
    body
        table
            tr
                th file
                th description
            each file in files
                +FileFormat(file)